{
	"$schema": "vscode://schemas/color-theme",
	"name": "Enterprise COBOL (for entcobol dialect)",
	"type": "dark",
	"colors": {
		"editor.background": "#000000",
		"editorCursor.foreground": "#F8F8F0",
		"editor.foreground": "#cfcfcf",
		"editorWhitespace.foreground": "#3B3A32",
		"editor.lineHighlightBackground": "#3E3D32",
		"editor.selectionBackground": "#49483E",
		"activityBar.background": "#1A1A1A",
		"activityBar.foreground": "#AE81FF",
		"activityBar.inactiveForeground": "#707070",
		"activityBarBadge.background": "#AE81FF",
		"activityBarBadge.foreground": "#000000",
		"sideBar.background": "#0A0A0A",
		"sideBar.foreground": "#cfcfcf",
		"sideBar.border": "#3B3A32",
		"sideBarTitle.foreground": "#AE81FF",
		"sideBarSectionHeader.background": "#1A1A1A",
		"sideBarSectionHeader.foreground": "#AE81FF",
		"statusBar.background": "#1A1A1A",
		"statusBar.foreground": "#AE81FF",
		"statusBar.border": "#3B3A32",
		"statusBar.debuggingBackground": "#27b9b1",
		"statusBar.debuggingForeground": "#000000",
		"titleBar.activeBackground": "#000000",
		"titleBar.activeForeground": "#cfcfcf",
		"titleBar.inactiveBackground": "#0A0A0A",
		"titleBar.inactiveForeground": "#707070",
		"titleBar.border": "#3B3A32",
		"editorLineNumber.foreground": "#555555",
		"editorLineNumber.activeForeground": "#AE81FF",
		"editorIndentGuide.background": "#3B3A32",
		"editorIndentGuide.activeBackground": "#49483E",
		"editorRuler.foreground": "#3B3A32",
		"editorBracketMatch.background": "#49483E80",
		"editorBracketMatch.border": "#AE81FF",
		"editor.selectionHighlightBackground": "#49483E80",
		"editor.wordHighlightBackground": "#49483E80",
		"editor.wordHighlightStrongBackground": "#3E3D3280",
		"editor.findMatchBackground": "#eeed1780",
		"editor.findMatchHighlightBackground": "#eeed1740",
		"editorError.foreground": "#df1190",
		"editorWarning.foreground": "#eeed17",
		"editorInfo.foreground": "#7da2fa",
		"editorHint.foreground": "#27b9b1",
		"editorWidget.background": "#0A0A0A",
		"editorWidget.border": "#3B3A32",
		"editorSuggestWidget.background": "#0A0A0A",
		"editorSuggestWidget.border": "#3B3A32",
		"editorSuggestWidget.foreground": "#cfcfcf",
		"editorSuggestWidget.selectedBackground": "#49483E",
		"editorSuggestWidget.highlightForeground": "#AE81FF",
		"panel.background": "#000000",
		"panel.border": "#3B3A32",
		"panelTitle.activeBorder": "#AE81FF",
		"panelTitle.activeForeground": "#cfcfcf",
		"panelTitle.inactiveForeground": "#707070",
		"terminal.foreground": "#cfcfcf",
		"terminal.ansiBlack": "#000000",
		"terminal.ansiBlue": "#7da2fa",
		"terminal.ansiCyan": "#27b9b1",
		"terminal.ansiGreen": "#95faab",
		"terminal.ansiMagenta": "#AE81FF",
		"terminal.ansiRed": "#df1190",
		"terminal.ansiWhite": "#cfcfcf",
		"terminal.ansiYellow": "#eeed17",
		"terminal.ansiBrightBlack": "#555555",
		"terminal.ansiBrightBlue": "#A0C0FF",
		"terminal.ansiBrightCyan": "#5FE0D8",
		"terminal.ansiBrightGreen": "#C0FFCC",
		"terminal.ansiBrightMagenta": "#D0A0FF",
		"terminal.ansiBrightRed": "#FF50B8",
		"terminal.ansiBrightWhite": "#F8F8F0",
		"terminal.ansiBrightYellow": "#FFFF80",
		"gitDecoration.modifiedResourceForeground": "#7da2fa",
		"gitDecoration.deletedResourceForeground": "#df1190",
		"gitDecoration.untrackedResourceForeground": "#27b9b1",
		"gitDecoration.ignoredResourceForeground": "#555555",
		"gitDecoration.conflictingResourceForeground": "#eeed17",
		"button.background": "#AE81FF",
		"button.foreground": "#000000",
		"button.hoverBackground": "#D0A0FF",
		"dropdown.background": "#0A0A0A",
		"dropdown.border": "#3B3A32",
		"input.background": "#0A0A0A",
		"input.border": "#3B3A32",
		"input.foreground": "#cfcfcf",
		"list.activeSelectionBackground": "#49483E",
		"list.activeSelectionForeground": "#cfcfcf",
		"list.hoverBackground": "#3E3D32",
		"list.inactiveSelectionBackground": "#3E3D32",
		"list.highlightForeground": "#AE81FF",
		"scrollbarSlider.background": "#3B3A3280",
		"scrollbarSlider.hoverBackground": "#49483E80",
		"scrollbarSlider.activeBackground": "#AE81FF80",
		"badge.background": "#AE81FF",
		"badge.foreground": "#000000",
		"progressBar.background": "#AE81FF"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#555555"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#eeed17"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#7da2fa"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"foreground": "#7da2fa"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "",
				"foreground": "#7da2fa"
			}
		},
		{
			"name": "Storage type",
			"scope": "storage.type",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#df1190"
			}
		},
		{
			"name": "Intrinsic functions",
			"scope": "keyword.intrinsicfunctions.cobol",
			"settings": {
				"fontStyle": "",
				"foreground": "#27b9b1"
			}
		},
		{
			"name": "built-in variables",
			"scope": "variable.language",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "Variable",
			"scope": "variable.cobol",
			"settings": {
				"fontStyle": "",
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"fontStyle": "",
				"foreground": "#27b9b1"
			}
		},
		{
			"name": "Compiler directing statements",
			"scope": "keyword.listing.cobol keyword.compiler.cobol",
			"settings": {
				"fontStyle": "",
				"foreground": "#27b9b1"
			}
		},
		{
			"name": "flow/control statements",
			"scope": "keyword.control.cobol",
			"settings": {
				"fontStyle": "",
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "statements",
			"scope": "keyword.verb keyword.verbs",
			"settings": {
				"fontStyle": "",
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "CICS API",
			"scope": "keyword.verb.cics",
			"settings": {
				"fontStyle": "",
				"foreground": "#0dac0d"
			}
		},
		{
			"name": "CICS invocation",
			"scope": "meta.symbol.cobol.cics",
			"settings": {
				"fontStyle": "",
				"foreground": "#95faab"
			}
		},
		{
			"name": "Db/2 API",
			"scope": "keyword.verb.db2",
			"settings": {
				"fontStyle": "",
				"foreground": "#2411d1"
			}
		},
		{
			"name": "Db/2 invocation",
			"scope": "meta.symbol.cobol.db2",
			"settings": {
				"fontStyle": "",
				"foreground": "#6ac2f5"
			}
		},
		{
			"name": "remarks",
			"scope": "keyword.cobol",
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		{
			"name": "JSON-XML",
			"scope": "keyword.json keyword.xml",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "sort",
			"scope": "keyword.cobol.sort",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "other statements",
			"scope": "keyword.other.cobol keyword.otherverbs.cobol keyword.identifiers.cobol",
			"settings": {
				"fontStyle": "",
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "operators/brackets",
			"scope": "keyword.operator.cobol keyword.bracket.cobol",
			"settings": {
				"fontStyle": "",
				"foreground": "#F8F8F2A5"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid keyword.reserved.cobol",
			"settings": {
				"fontStyle": "",
				"foreground": "#27b9b1"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#27b9b1"
			}
		},
		{
			"name": "metasybmols",
			"scope": "meta.symbol keyword.filler.cobol",
			"settings": {
				"foreground": "#eeed17"
			}
		}
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"variable.readonly:COBOL": {
			"bold": true
		}
	}
}