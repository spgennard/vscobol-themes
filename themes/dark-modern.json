{
	"$schema": "vscode://schemas/color-theme",
	"name": "Dark Green Modern",
	"type": "dark",
	"colors": {
		"editor.background": "#000000",
		"editor.foreground": "#3A8D32",
		"activityBar.background": "#0D1D0D",
		"activityBar.foreground": "#86DA64",
		"activityBar.inactiveForeground": "#3A8D32",
		"activityBarBadge.background": "#86DA64",
		"activityBarBadge.foreground": "#000000",
		"sideBar.background": "#0A0A0A",
		"sideBar.foreground": "#89EDA0",
		"sideBar.border": "#1A3D1A",
		"sideBarTitle.foreground": "#86DA64",
		"sideBarSectionHeader.background": "#0D1D0D",
		"sideBarSectionHeader.foreground": "#86DA64",
		"statusBar.background": "#0D1D0D",
		"statusBar.foreground": "#86DA64",
		"statusBar.border": "#1A3D1A",
		"statusBar.debuggingBackground": "#f6f79D",
		"statusBar.debuggingForeground": "#000000",
		"titleBar.activeBackground": "#000000",
		"titleBar.activeForeground": "#3A8D32",
		"titleBar.inactiveBackground": "#0A0A0A",
		"titleBar.inactiveForeground": "#27725E",
		"titleBar.border": "#1A3D1A",
		"editorLineNumber.foreground": "#27725E",
		"editorLineNumber.activeForeground": "#86DA64",
		"editorCursor.foreground": "#86DA64",
		"editorWhitespace.foreground": "#1A3D1A",
		"editorIndentGuide.background": "#1A3D1A",
		"editorIndentGuide.activeBackground": "#27725E",
		"editorRuler.foreground": "#1A3D1A",
		"editorBracketMatch.background": "#1A3D1A80",
		"editorBracketMatch.border": "#86DA64",
		"editor.selectionBackground": "#1A3D1A",
		"editor.selectionHighlightBackground": "#1A3D1A80",
		"editor.lineHighlightBackground": "#0D1D0D",
		"editor.wordHighlightBackground": "#1A3D1A80",
		"editor.wordHighlightStrongBackground": "#27725E80",
		"editor.findMatchBackground": "#f6f79D80",
		"editor.findMatchHighlightBackground": "#f6f79D40",
		"editorError.foreground": "#E5004C",
		"editorWarning.foreground": "#F48B34",
		"editorInfo.foreground": "#2FD6C3",
		"editorHint.foreground": "#86DA64",
		"editorWidget.background": "#0A0A0A",
		"editorWidget.border": "#1A3D1A",
		"editorSuggestWidget.background": "#0A0A0A",
		"editorSuggestWidget.border": "#1A3D1A",
		"editorSuggestWidget.foreground": "#3A8D32",
		"editorSuggestWidget.selectedBackground": "#1A3D1A",
		"editorSuggestWidget.highlightForeground": "#86DA64",
		"panel.background": "#000000",
		"panel.border": "#1A3D1A",
		"panelTitle.activeBorder": "#86DA64",
		"panelTitle.activeForeground": "#86DA64",
		"panelTitle.inactiveForeground": "#27725E",
		"terminal.foreground": "#3A8D32",
		"terminal.ansiBlack": "#000000",
		"terminal.ansiBlue": "#2FD6C3",
		"terminal.ansiCyan": "#2FD6C3",
		"terminal.ansiGreen": "#86DA64",
		"terminal.ansiMagenta": "#ff60AD",
		"terminal.ansiRed": "#E5004C",
		"terminal.ansiWhite": "#ECEEEF",
		"terminal.ansiYellow": "#f6f79D",
		"terminal.ansiBrightBlack": "#27725E",
		"terminal.ansiBrightBlue": "#89EDA0",
		"terminal.ansiBrightCyan": "#7FFFEA",
		"terminal.ansiBrightGreen": "#B5FF8A",
		"terminal.ansiBrightMagenta": "#FF8FD8",
		"terminal.ansiBrightRed": "#FF5070",
		"terminal.ansiBrightWhite": "#FFFFFF",
		"terminal.ansiBrightYellow": "#FFFFC0",
		"gitDecoration.modifiedResourceForeground": "#f6f79D",
		"gitDecoration.deletedResourceForeground": "#E5004C",
		"gitDecoration.untrackedResourceForeground": "#2FD6C3",
		"gitDecoration.ignoredResourceForeground": "#27725E",
		"gitDecoration.conflictingResourceForeground": "#F48B34",
		"button.background": "#86DA64",
		"button.foreground": "#000000",
		"button.hoverBackground": "#89EDA0",
		"dropdown.background": "#0A0A0A",
		"dropdown.border": "#1A3D1A",
		"input.background": "#0A0A0A",
		"input.border": "#1A3D1A",
		"input.foreground": "#3A8D32",
		"list.activeSelectionBackground": "#1A3D1A",
		"list.activeSelectionForeground": "#86DA64",
		"list.hoverBackground": "#0D1D0D",
		"list.inactiveSelectionBackground": "#0D1D0D",
		"list.highlightForeground": "#86DA64",
		"scrollbarSlider.background": "#1A3D1A80",
		"scrollbarSlider.hoverBackground": "#27725E80",
		"scrollbarSlider.activeBackground": "#86DA6480",
		"badge.background": "#86DA64",
		"badge.foreground": "#000000",
		"progressBar.background": "#86DA64"
	},
	"tokenColors": [
		{
			"scope": [
				"comment",
				"comment.line",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#ff60AD"
			}
		},
		{
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#2FD6C3"
			}
		},
		{
			"scope": [
				"keyword"
			],
			"settings": {
				"foreground": "#86DA64"
			}
		},
		{
			"scope": [
				"storage"
			],
			"settings": {
				"foreground": "#86DA64"
			}
		},
		{
			"scope": [
				"keyword.control",
				"constant.other.color",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#86DA64",
				"fontStyle": "italic bold"
			}
		},
		{
			"scope": [
				"entity.name.function",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#f6f79D"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#FCDB1F"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#F48B34"
			}
		},
		{
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#E5004C"
			}
		},
		{
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#BDBEBC",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#ECEEEF"
			}
		},
		{
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#89EDA0",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"support.type.property-name",
				"meta.structure.dictionary.value"
			],
			"settings": {
				"foreground": "#86DA64"
			}
		},
		{
			"scope": [
				"string.quoted.double"
			],
			"settings": {
				"foreground": "#BDBEBC"
			}
		}
		],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"variable.readonly:COBOL": {
			"bold": true
		}
	}
}