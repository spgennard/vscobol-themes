{
	"$schema": "vscode://schemas/color-theme",
	"name": "Throwback (NX Inspired)",
	"type": "light",
	"colors": {
		"editor.background": "#ffffff",
		"editor.foreground": "#000000",
		"activityBar.background": "#F0F0F0",
		"activityBar.foreground": "#0000C0",
		"activityBar.inactiveForeground": "#707070",
		"activityBarBadge.background": "#0000C0",
		"activityBarBadge.foreground": "#ffffff",
		"sideBar.background": "#F8F8F8",
		"sideBar.foreground": "#000000",
		"sideBar.border": "#E0E0E0",
		"sideBarTitle.foreground": "#0000C0",
		"sideBarSectionHeader.background": "#F0F0F0",
		"sideBarSectionHeader.foreground": "#0000C0",
		"statusBar.background": "#F0F0F0",
		"statusBar.foreground": "#0000C0",
		"statusBar.border": "#E0E0E0",
		"statusBar.debuggingBackground": "#8D1A1A",
		"statusBar.debuggingForeground": "#ffffff",
		"titleBar.activeBackground": "#ffffff",
		"titleBar.activeForeground": "#000000",
		"titleBar.inactiveBackground": "#F8F8F8",
		"titleBar.inactiveForeground": "#707070",
		"titleBar.border": "#E0E0E0",
		"editorLineNumber.foreground": "#9CA0A4",
		"editorLineNumber.activeForeground": "#0000C0",
		"editorCursor.foreground": "#000000",
		"editorWhitespace.foreground": "#D0D0D0",
		"editorIndentGuide.background": "#E0E0E0",
		"editorIndentGuide.activeBackground": "#C0C0C0",
		"editorRuler.foreground": "#E0E0E0",
		"editorBracketMatch.background": "#D0D0D080",
		"editorBracketMatch.border": "#0000C0",
		"editor.selectionBackground": "#C0D8FF",
		"editor.selectionHighlightBackground": "#E0ECFF",
		"editor.lineHighlightBackground": "#F8F8F8",
		"editor.wordHighlightBackground": "#E0ECFF80",
		"editor.wordHighlightStrongBackground": "#C0D8FF80",
		"editor.findMatchBackground": "#FFD70080",
		"editor.findMatchHighlightBackground": "#FFED0040",
		"editorError.foreground": "#8D1A1A",
		"editorWarning.foreground": "#FF9900",
		"editorInfo.foreground": "#0000C0",
		"editorHint.foreground": "#707070",
		"editorWidget.background": "#F8F8F8",
		"editorWidget.border": "#E0E0E0",
		"editorSuggestWidget.background": "#F8F8F8",
		"editorSuggestWidget.border": "#E0E0E0",
		"editorSuggestWidget.foreground": "#000000",
		"editorSuggestWidget.selectedBackground": "#C0D8FF",
		"editorSuggestWidget.highlightForeground": "#0000C0",
		"panel.background": "#ffffff",
		"panel.border": "#E0E0E0",
		"panelTitle.activeBorder": "#0000C0",
		"panelTitle.activeForeground": "#000000",
		"panelTitle.inactiveForeground": "#707070",
		"terminal.foreground": "#000000",
		"terminal.ansiBlack": "#000000",
		"terminal.ansiBlue": "#0000C0",
		"terminal.ansiCyan": "#0080C0",
		"terminal.ansiGreen": "#008000",
		"terminal.ansiMagenta": "#880088",
		"terminal.ansiRed": "#8D1A1A",
		"terminal.ansiWhite": "#E0E0E0",
		"terminal.ansiYellow": "#FF9900",
		"terminal.ansiBrightBlack": "#707070",
		"terminal.ansiBrightBlue": "#0000FF",
		"terminal.ansiBrightCyan": "#00BBFF",
		"terminal.ansiBrightGreen": "#00BB00",
		"terminal.ansiBrightMagenta": "#BB00BB",
		"terminal.ansiBrightRed": "#CC2020",
		"terminal.ansiBrightWhite": "#ffffff",
		"terminal.ansiBrightYellow": "#FFBB00",
		"gitDecoration.modifiedResourceForeground": "#0000C0",
		"gitDecoration.deletedResourceForeground": "#8D1A1A",
		"gitDecoration.untrackedResourceForeground": "#008000",
		"gitDecoration.ignoredResourceForeground": "#B0B0B0",
		"gitDecoration.conflictingResourceForeground": "#FF9900",
		"button.background": "#0000C0",
		"button.foreground": "#ffffff",
		"button.hoverBackground": "#0000FF",
		"dropdown.background": "#ffffff",
		"dropdown.border": "#E0E0E0",
		"input.background": "#ffffff",
		"input.border": "#E0E0E0",
		"input.foreground": "#000000",
		"list.activeSelectionBackground": "#C0D8FF",
		"list.activeSelectionForeground": "#000000",
		"list.hoverBackground": "#F0F0F0",
		"list.inactiveSelectionBackground": "#F0F0F0",
		"list.highlightForeground": "#0000C0",
		"scrollbarSlider.background": "#C0C0C080",
		"scrollbarSlider.hoverBackground": "#A0A0A080",
		"scrollbarSlider.activeBackground": "#80808080",
		"badge.background": "#0000C0",
		"badge.foreground": "#ffffff",
		"progressBar.background": "#0000C0"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#B0B0B0"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#B0B0B0"
			}
		},
		{
			"name": "COBOL: Meta",
			"scope": [
				"meta.symbol"
			],
			"settings": {
				"foreground": "#8D1A1A"
			}
		},
		{
			"name": "JCL: Meta",
			"scope": [
				"meta.symbol.jcl"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Invalid - Deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"fontStyle": "bold underline",
				"foreground": "#ff0000"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ff0000"
			}
		},
		{
			"name": "Control keywords",
			"scope": "keyword.control",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"foreground": "#2B952B"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#2B952B"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language"
			],
			"settings": {
				"foreground": "#80111F"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				"foreground": "#7A3E9D"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#AA3731"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#7A3E9D"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Sections",
			"scope": "entity.name.section",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Numbers, Characters",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Strings: Symbols",
			"scope": "constant.other.symbol",
			"settings": {
				"foreground": "#9E1D9D"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "HTML: Doctype Declaration",
			"scope": [
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype string",
				"meta.tag.sgml.doctype entity.name.tag",
				"meta.tag.sgml punctuation.definition.tag.html"
			],
			"settings": {
				"foreground": "#AAAAAA"
			}
		},
		{
			"name": "HTML: Tags",
			"scope": [
				"meta.tag",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#91B3E0"
			}
		},
		{
			"name": "HTML: Tag Names",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "HTML: Attribute Names",
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#91B3E0"
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "CSS: Selectors",
			"scope": [
				"meta.selector",
				"meta.selector entity",
				"meta.selector entity punctuation",
				"entity.name.tag.css"
			],
			"settings": {
				"foreground": "#7A3E9D"
			}
		},
		{
			"name": "CSS: Property Names",
			"scope": [
				"meta.property-name",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "CSS: Property Values",
			"scope": [
				"meta.property-value",
				"meta.property-value constant.other",
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "CSS: Important Keyword",
			"scope": "keyword.other.important",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: Error",
			"scope": "markup.error",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Markup: Insertion",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Link",
			"scope": "meta.link",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Markup: Output",
			"scope": [
				"markup.output",
				"markup.raw"
			],
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Markup: Prompt",
			"scope": "markup.prompt",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Markup: Heading",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#AA3731"
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Traceback",
			"scope": "markup.traceback",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Markup: Underline",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#9E1D9D"
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"fontStyle": "",
				"foreground": "#AB6526"
			}
		},
		{
			"name": "Extra: Diff Range",
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff From",
			"scope": "meta.diff.header.from-file",
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff To",
			"scope": "meta.diff.header.to-file",
			"settings": {
				"foreground": "#434343"
			}
		}
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"variable.readonly:COBOL": {
			"bold": true
		}
	}
}