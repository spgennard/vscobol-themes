{
    "name": "cobol-themes",
    "displayName": "COBOL Themes Pack",
    "description": "COBOL Themes (Eclipse, NX, ISPF, EntCOBOL) Inspired",
    "version": "1.0.2",
    "publisher": "bitlang",
    "engines": {
        "vscode": "^1.45.0"
    },
    "icon": "images/vscode_themes.png",
    "categories": [
        "Themes"
    ],
    "repository": {
        "type": "git",
        "url" : "https://github.com/spgennard/vscobol-themes"
    },
    "contributes": {
        "themes": [
            {
                "label": "Throwback (Eclipse Inspired)",
                "uiTheme": "vs",
                "path": "./themes/throwback-light-theme.json"
            },
            {
                "label": "Throwback (NX Inspired)",
                "uiTheme": "vs",
                "path": "./themes/throwback-light-nx-theme.json"
            },
            {
                "label": "Throwback (ISPF Inspired)",
                "uiTheme": "vs-dark",
                "path": "./themes/throwback-green-screen-theme.json"
            },
            {
                "label": "Enterprise COBOL (for entcobol dialect)",
                "uiTheme": "vs-dark",
                "path": "./themes/entcob-color-theme.json"
            }

        ]
    }
}